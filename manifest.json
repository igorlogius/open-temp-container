{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/ephemeral-containers",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_area": "navbar",

    "theme_icons": [
      {
        "light": "dark.png",
        "dark": "light.png",
        "size": 512
      }
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "description": "Toolbar Action"
    },
    "opennewtab": {
      "description": "Open newtab in new ephemeral container"
    },
    "openinsame": {
      "description": "Open newtab in same ephemeral container as the active tab"
    }
  },
  "description": "Open tabs in ephemeral containers",
  "icons": {
    "512": "icon.svg"
  },
  "manifest_version": 2,
  "name": "Ephemeral Containers",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "<all_urls>",
    "contextualIdentities",
    "cookies",
    "menus",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "optional_permissions": ["bookmarks", "history"],
  "version": "1.2.53"
}
